{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "assignees": ["kymzky"],
  "reviewers": ["kymzky"],
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchPackageNames": [
        "ghcr.io/hotio/jellyseerr"
      ],
      "versioning": "regex:^(?<compatibility>release-)?(?<major>\\d+)\\.(?<minor>\\d+)\\.(?<patch>\\d+)$"
    },
    {
      "matchPackageNames": [
        "ghcr.io/wg-easy/wg-easy"
      ],
      "versioning": "regex:^(?<major>\\d+)$"
    }
  ]
}
